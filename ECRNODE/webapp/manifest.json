{
	"_version": "1.1.0",
	"sap.app": {
		"id": "firstusertask",
		"_version": "1.3.0",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{APPTITLE}}",
		"description": "{{APPDESCRIPTION}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"WORKFLOWLOG.xsodata": {
				"uri": "/html5apps/aplexworkflow/destinations/APLEXHANA/WORKFLOWLOG.xsodata/",
				"type": "OData",
				"settings": {
					"annotations": []
				}
			},
			"API_CV_ATTACHMENT_SRV": {
				"uri": "/html5apps/aplexworkflow/destinations/WT_S4HC/API_CV_ATTACHMENT_SRV/",
				"type": "OData",
				"settings": {
					"annotations": []
				}
			},
			"EMPLOYEES.xsodata": {
				"uri": "/html5apps/aplexworkflow/destinations/APLEXHANA/EMPLOYEES.xsodata/",
				"type": "OData",
				"settings": {
					"annotations": []
				}
			},
			"ECROdata.xsodata": {
				"uri": "/html5apps/aplexworkflow/destinations/APLEXHANA/ECROdata.xsodata/",
				"type": "OData",
				"settings": {
					"annotations": []
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "ECRNODE.view.App",
			"type": "XML",
			"async": false,
			"id": "idAppControl"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ECRNODE.i18n.i18n"
				}
			},
			"Attach": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"useBatch": true
				},
				"dataSource": "API_CV_ATTACHMENT_SRV",
				"preload": true
			},
			"SaveLOG": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"useBatch": true
				},
				"dataSource": "WORKFLOWLOG.xsodata",
				"preload": true
			},
			"userAttributes": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": false,
				"uri": "/html5apps/ecrapproval/services/userapi/attributes"
			},
			"GetEMPLOYEES": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"useBatch": true
				},
				"dataSource": "EMPLOYEES.xsodata",
				"preload": true
			},
			"ECR": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"useBatch": false,
					"defaultUpdateMethod": "Put"
				},
				"dataSource": "ECROdata.xsodata",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	},
	"sap.platform.hcp": {
		"uri": "ECRNODE/webapp",
		"_version": "1.1.0"
	}
}